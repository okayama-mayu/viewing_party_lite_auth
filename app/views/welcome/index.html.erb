<h1>Viewing Party Lite</h1>

<% if current_user %>
  <%= link_to 'Log Out', '/logout', method: :delete %>
<% else %>
  <p><%= button_to 'Create a New User', '/register', method: :get %></p>
  <%= link_to 'Log In', login_path %>
<% end %>

<h2>Existing Users</h2>

<% @users.each do |user| %>
  <p><%= link_to "#{user.name}", user_path(user) %></p>
<% end %>